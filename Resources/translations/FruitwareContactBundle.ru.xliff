<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="fruitware_contact.form_page_title">
                <source>fruitware_contact.form_page_title</source>
                <target>Обратная связь</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form_title">
                <source>fruitware_contact.form_title</source>
                <target>Свяжитесь с нами</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form.first_name">
                <source>fruitware_contact.form.first_name</source>
                <target>Имя</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form.last_name">
                <source>fruitware_contact.form.last_name</source>
                <target>Фамилия</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form.email">
                <source>fruitware_contact.form.email</source>
                <target>Email</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form.subject">
                <source>fruitware_contact.form.subject</source>
                <target>Тема</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form.message">
                <source>fruitware_contact.form.message</source>
                <target>Сообщение</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form.captcha">
                <source>fruitware_contact.form.captcha</source>
                <target>Введите код</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.form_submit">
                <source>fruitware_contact.form_submit</source>
                <target>Отправить</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.email_message_txt">
                <source>fruitware_contact.email_message_txt</source>
                <target><![CDATA[Здравствуйте,

Пользователь заполнил контактную форму:

Имя: %firstName%

Фамилия: %lastName%

Email: %email%

Сообщение:
%message%
]]></target>
            </trans-unit>
            <trans-unit id="fruitware_contact.email_message_html">
                <source>fruitware_contact.email_message_html</source>
                <target><![CDATA[Здравствуйте,
<br/>
<br/>
Пользователь заполнил контактную форму:
<br/>
<br/>
<strong><em>Имя:</em></strong> %firstName%
<br/>
<br/>
<strong><em>Фамилия:</em></strong> %lastName%
<br/>
<br/>
<strong><em>Email:</em></strong> %email%
<br/>
<br/>
<strong><em>Сообщение:</em></strong>
<br/>
%message%
]]></target>
            </trans-unit>
            <trans-unit id="fruitware_contact.email_default_subject">
                <source>fruitware_contact.email_default_subject</source>
                <target>Форма контактов</target>
            </trans-unit>

            <trans-unit id="fruitware_contact.confirm_page_title">
                <source>fruitware_contact.confirm_page_title</source>
                <target>Форма успешно отослана</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.confirm_title">
                <source>fruitware_contact.confirm_title</source>
                <target>Спасибо %fullName%</target>
            </trans-unit>
            <trans-unit id="fruitware_contact.confirm_message">
                <source>fruitware_contact.confirm_message</source>
                <target>Ваш запрос был успешно отправлен! Спасибо большое за проявленный интерес.</target>
            </trans-unit>
        </body>
    </file>
</xliff>
